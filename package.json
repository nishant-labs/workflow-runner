{
  "name": "@nishant-labs/run-it",
  "version": "0.1.0",
  "description": "A feature-rich CLI tool built with CommanderJS",
  "type": "commonjs",
  "main": "dist/index.js",
  "bin": {
    "runit": "./bin/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "vite build",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky"
  },
  "keywords": [
    "cli",
    "commander",
    "typescript"
  ],
  "author": "nishant-labs",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "7.9.0",
    "chalk": "5.6.2",
    "cli-table3": "0.6.5",
    "commander": "14.0.1",
    "ora": "9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@types/node": "20.19.22",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "3.6.2",
    "pretty-quick": "^4.2.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1",
    "vite": "7.1.11"
  },
  "lint-staged": {
    "*": [
      "npm run lint",
      "pretty-quick --staged"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "bin",
    "dist"
  ]
}
